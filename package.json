{
    "name": "robot-manager-frontend",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "NODE_ENV=production ts-node src/server/main.js",
        "dev": "nodemon src/server/main.js -w src/server",
        "build": "vite build",
        "prepare": "husky install"
    },
    "lint-staged": {
        "src/**/*.{html,js,ts,scss}": [
            "prettier --write"
        ],
        "src/**/*.ts": [
            "eslint --fix"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "dependencies": {
        "@blueprintjs/core": "^5.0.0-beta.0",
        "@blueprintjs/icons": "^5.0.0-beta.0",
        "connect-redis": "^7.1.0",
        "cookie-parser": "^1.4.6",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-http-proxy": "^1.6.3",
        "express-session": "^1.17.3",
        "https": "^1.0.0",
        "passport": "^0.6.0",
        "passport-onshape": "^1.1.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.15.0",
        "redis": "^4.6.8",
        "sass": "^1.63.4",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5",
        "uuid": "^9.0.0",
        "vite-express": "^0.10.0",
        "vite-plugin-svgr": "^3.2.0"
    },
    "devDependencies": {
        "@types/react": "^18.2.7",
        "@types/react-dom": "^18.2.4",
        "@vitejs/plugin-react": "^4.0.0",
        "husky": "^8.0.0",
        "lint-staged": "^14.0.1",
        "nodemon": "^3.0.1",
        "prettier": "^3.0.3",
        "vite": "^4.3.9"
    }
}
